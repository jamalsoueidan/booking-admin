{
  "name": "booking-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:prod": "API_URL=https://booking-shopify-api.azurewebsites.net/api npm run dev",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "storybook": "storybook dev -p 6006",
    "prebuild-storybook": "npm run fb-openapi",
